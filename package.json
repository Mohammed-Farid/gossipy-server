{
  "name": "gossipy-server",
  "author": "Mohammed Farid",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "heroku-postbuild": "prisma generate && npm run tsc",
    "tsc": "tsc",
    "start": "node ./lib/index.js",
    "dev": "env-cmd -f ./config/prod.env ts-node-dev ./src/index.ts",
    "seed": "env-cmd -f ./config/prod.env jest seedProd.test.ts",
    "test": "env-cmd -f ./config/test.env jest --watch --runInBand",
    "prod:gene": "prisma generate -e ./config/prod.env",
    "prod:deploy": "prisma deploy -e ./config/prod.env",
    "prod:token": "prisma token -c -e ./config/prod.env",
    "prod:reset": "prisma reset -e ./config/prod.env",
    "test:deploy": "prisma deploy -e ./config/test.env",
    "test:reset": "prisma reset -e ./config/test.env"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/ws": "^7.2.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.3.6",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.1",
    "jest": "^25.2.7",
    "ts-jest": "^25.3.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^3.0.0",
    "env-cmd": "^10.0.1",
    "graphql": "^14.5.8",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "prisma": "^1.34.10",
    "prisma-binding": "^2.3.16",
    "prisma-client-lib": "^1.34.10",
    "ts-node": "^8.6.2"
  }
}
